{
	"$schema": "https://turborepo.org/schema.json",
	"tasks": {
		"build": {
			"dependsOn": [],
			"outputs": ["dist/**/*", "docs/**/*"]
		},
		"bundle": {
			"dependsOn": [],
			"outputs": ["storybook_static/**/*"]
		},
		"test": {
			"dependsOn": [],
			"outputs": ["coverage/**/*"]
		},
		"ci": {
			"dependsOn": ["test", "check-deps", "build"],
			"outputs": []
		},
		"check-deps": {
			"dependsOn": [],
			"outputs": []
		},
		"clean": {
			"cache": false
		},
		"release": {
			"cache": false
		},
		"start": {
			"cache": false
		},
		"stories": {
			"cache": false
		}
	}
}
